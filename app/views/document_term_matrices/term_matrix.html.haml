%h1= "Term Document Matrix"
= "Dictionary Size: #{$dictionary.size}"
%table
  %tr
    %td
    - $dictionary.each do |key, value|
      %td
        %strong= key
        %br
        = value[0]
        %br
        = value[1]
  - @term_vectors.each_index do |i|
    %tr
      %td= i
      - @term_vectors[i].each do |value|
        %td= value
        